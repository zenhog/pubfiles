# Shows pseudocode in disassembly. Eg mov eax, str.ok = > eax = str.ok
e asm.pseudo = false
e bin.cache = true
e asm.syntax = att

#ESIL EMU
e asm.emu = true
e asm.esil = false

# Display stack and register values on top of disasembly view (visual mode)
e cmd.stack = true

# Use UTF-8 to show cool arrows that do not look like crap :)
e scr.utf8 = true

e asm.describe = true

# Sanbox settings
e cfg.sandbox = false

# Dark theme
eco dark
